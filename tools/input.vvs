; fib sequence
.start
    uload r0 0 ; prev
    uload r1 1 ; cur
    uload r2 1 ; next
    uload r3 1 ; inc
    uload r4 2 ; counter
    uload r5 10 ; to find

    label loop
        ucmp r4 r5
        jg exit


        uload r0 0
        uadd r0 r1 # prev = cur

        uload r1 0
        uadd r1 r2 # cur = next

        uload r2 0
        uadd r2 r1 # next = cur
        uadd r2 r0 # .. + prev


        uadd r4 r3
        jmp loop

    label exit
    ncall 1 r1
    halt
